<h3>イベントチケット情報一覧</h3>
<div>
  <%= link_to new_admin_event_ticket_path do %>
    <button type="button">+</button>
  <% end %>
</div>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>イベントチケット名</th><br>
      <th>ジャンル名</th>
      <th>販売ステータス</th>
    </tr>
  </thead>
  <tbody>
    <% @eventtickets.each do |event_ticket| %>
      <tr>
        <td><%= event_ticket.id %></td>
        <td><%= link_to event_ticket.ticket_name, admin_event_ticket_path(event_ticket) %></td>
        <td><%= event_ticket.genre.name %></td>
        <td>
          <% if event_ticket.sales_status %>
            販売中
          <% else %>
            販売完了
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @eventtickets %>