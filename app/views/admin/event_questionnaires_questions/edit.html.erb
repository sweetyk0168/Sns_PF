<div>
  <h3>イベントチケット情報編集</h3>
  <%= form_for(@event_questionnaires_question, url: admin_event_questionnaires_question_path) do |f| %>
    <%= f.label :"イベントアンケート名" %>
    <%= f.text_field:title %><br>
    <%= f.label :"イベントアンケート説明文" %>
    <%= f.text_area:content %><br>
    <%= f.submit '変更を保存' %>
  <% end %>
</div>